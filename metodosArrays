let ciudades = ["berlin", "Madrid", "Singapur", "Zacatecas"];

function adCity(city) {
  city = prompt("Cual es tu ciudad favorita?");
  ciudades.push(city);
  for (cities of ciudades) {
    alert(ciudades);
    break;
  }
}

let dislikeCity;
function deleteCity(dislike) {
  dislike = prompt("Que ciudad no deberia estar en el top?, la eliminaremos.").toUpperCase;
  dislikeCity = ciudades.filter((citie) => citie !== dislike);
  alert(dislikeCity);
alert(ciudades);

adCity();
deleteCity();
